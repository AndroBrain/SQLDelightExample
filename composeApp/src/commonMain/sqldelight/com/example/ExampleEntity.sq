CREATE TABLE ExampleEntity (
    id INTEGER NOT NULL PRIMARY KEY AUTOINCREMENT,
    name TEXT NOT NULL
);

insert:
INSERT OR REPLACE INTO ExampleEntity(id, name)
VALUES(?,?);

getAll:
SELECT * FROM ExampleEntity;

updateName:
UPDATE ExampleEntity
SET name = :name
WHERE id IS :id;

delete:
DELETE FROM ExampleEntity
WHERE id IS :id;
